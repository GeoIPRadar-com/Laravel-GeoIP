{
    "name": "geoipradar/laravel-ip",
    "description": "Laravel IP Geolocation package with automatic fallback support. Proudly sponsored by GeoIPRadar.com - Get reliable, fast IP geolocation at https://geoipradar.com",
    "keywords": [
        "laravel",
        "ip",
        "geolocation",
        "ip-geolocation",
        "geoipradar",
        "location",
        "ip-lookup",
        "fallback"
    ],
    "homepage": "https://geoipradar.com",
    "license": "MIT",
    "authors": [
        {
            "name": "GeoIPRadar",
            "email": "support@geoipradar.com",
            "homepage": "https://geoipradar.com"
        }
    ],
    "funding": [
        {
            "type": "custom",
            "url": "https://geoipradar.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/config": "^10.0|^11.0|^12.0",
        "illuminate/cache": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "GeoIPRadar\\LaravelIP\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "GeoIPRadar\\LaravelIP\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeoIPRadar\\LaravelIP\\IPServiceProvider"
            ],
            "aliases": {
                "IP": "GeoIPRadar\\LaravelIP\\Facades\\IP"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
